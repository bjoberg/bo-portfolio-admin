{
  "name": "bo-portfolio",
  "version": "4.3.0",
  "description": "Brett Oberg's portfolio website",
  "private": true,
  "main": "./build/server.js",
  "scripts": {
    "build": "npm run build:server && npm run build:webpack:server && npm run build:webpack:client",
    "build:server": "rm -rf server/dist && ./node_modules/.bin/tsc --project ./tsconfig.json",
    "build:webpack:client": "./node_modules/.bin/webpack --config ./config/webpack.client.config.js",
    "build:webpack:server": "./node_modules/.bin/webpack --config ./config/webpack.server.config.js",
    "dev": "./node_modules/.bin/tsc && concurrently \"./node_modules/.bin/tsc -w\" \"node ./server/dist/server.js\"",
    "lint": "npm run lint:client && npm run lint:server",
    "lint:client": "./node_modules/.bin/eslint src",
    "lint:server": "./node_modules/.bin/tslint --config ./tslint.json --project ./tsconfig.json",
    "start": "node ./build/server.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
